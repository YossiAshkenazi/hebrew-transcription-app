    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "express-rate-limit": "^6.7.1",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.1",
    "nodemailer": "^6.9.3",
    "axios": "^1.4.0",
    "form-data": "^4.0.0",
    "sequelize": "^6.32.1",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.7",
    "bull": "^4.11.3",
    "aws-sdk": "^2.1406.0",
    "uuid": "^9.0.0",
    "joi": "^17.9.2",
    "winston": "^3.9.0",
    "openai": "^4.0.0",
    "ffmpeg-static": "^5.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "speakeasy": "^2.0.0",
