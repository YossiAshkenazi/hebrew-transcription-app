    "nodemon": "^3.0.1",
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^3.0.0",
    "sequelize-cli": "^6.6.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
